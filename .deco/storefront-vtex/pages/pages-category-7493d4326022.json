{
  "name": "Category",
  "path": "/*",
  "default": { "possiblePaths": [] },
  "sections": [
    {
      "jsonLD": { "__resolveType": "PLP Loader.json" },
      "__resolveType": "commerce/sections/Seo/SeoPLP.tsx"
    },
    { "__resolveType": "Header - 01.json" },
    {
      "page": { "__resolveType": "PLP Loader.json" },
      "layout": {
        "columns": { "mobile": 2, "desktop": 4 },
        "variant": "aside",
        "pagination": "show-more"
      },
      "cardLayout": { "__resolveType": "Product Card Layout - 01.json" },
      "startingPage": 1,
      "__resolveType": "deco-sites/storefront/sections/Product/SearchResult.tsx"
    },
    { "__resolveType": "Footer - 01.json" }
  ],
  "__resolveType": "website/pages/Page.tsx"
}
